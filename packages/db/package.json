{
  "name": "@olis/db",
  "type": "module",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "default": "./dist/schema/index.js"
    },
    "./schema/platform": {
      "types": "./dist/schema/platform/index.d.ts",
      "default": "./dist/schema/platform/index.js"
    },
    "./schema/identity": {
      "types": "./dist/schema/identity/index.d.ts",
      "default": "./dist/schema/identity/index.js"
    },
    "./schema/marketplace": {
      "types": "./dist/schema/marketplace/index.d.ts",
      "default": "./dist/schema/marketplace/index.js"
    },
    "./schema/remodel-x": {
      "types": "./dist/schema/remodel-x/index.d.ts",
      "default": "./dist/schema/remodel-x/index.js"
    },
    "./seeds/platform": {
      "types": "./dist/seeds/platform/index.d.ts",
      "default": "./dist/seeds/platform/index.js"
    },
    "./seeds/identity": {
      "types": "./dist/seeds/identity/index.d.ts",
      "default": "./dist/seeds/identity/index.js"
    },
    "./seeds/marketplace": {
      "types": "./dist/seeds/marketplace/index.d.ts",
      "default": "./dist/seeds/marketplace/index.js"
    },
    "./seeds/remodel-x": {
      "types": "./dist/seeds/remodel-x/index.d.ts",
      "default": "./dist/seeds/remodel-x/index.js"
    },
    "./seeds/*": {
      "types": "./dist/seeds/*.d.ts",
      "default": "./dist/seeds/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./types": "./src/types/index.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:init-schemas": "dotenv -e .env tsx ./src/lib/init-schemas.ts",
    "db:push:platform": "dotenv -e .env drizzle-kit push --strict --verbose --schema=./src/schema/platform/index.ts --schemaFilter=platform --dialect=postgresql --url=$DATABASE_URL",
    "db:reset:platform": "dotenv -e .env tsx ./src/lib/db-reset.ts platform",
    "db:seed:platform": "dotenv -e .env tsx ./src/lib/db-seed.ts platform",
    "db:push:identity": "dotenv -e .env drizzle-kit push --strict --verbose --schema=./src/schema/identity/index.ts --schemaFilter=identity --dialect=postgresql --url=$DATABASE_URL",
    "db:reset:identity": "dotenv -e .env tsx ./src/lib/db-reset.ts identity",
    "db:seed:identity": "dotenv -e .env tsx ./src/lib/db-seed.ts identity",
    "db:push:marketplace": "dotenv -e .env drizzle-kit push --strict --verbose --schema=./src/schema/marketplace/index.ts --schemaFilter=marketplace --dialect=postgresql --url=$DATABASE_URL",
    "db:reset:marketplace": "dotenv -e .env tsx ./src/lib/db-reset.ts marketplace",
    "db:seed:marketplace": "dotenv -e .env tsx ./src/lib/db-seed.ts marketplace",
    "db:push:oss": "dotenv -e .env drizzle-kit push --strict --verbose --schema=./src/schema/remodel-x/index.ts --schemaFilter=one_stop_sales --dialect=postgresql --url=$DATABASE_URL",
    "db:reset:oss": "dotenv -e .env tsx ./src/lib/db-reset.ts oneStopSales",
    "db:seed:oss": "dotenv -e .env tsx ./src/lib/db-seed.ts oneStopSales"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@olis/core": "workspace:*",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "pg": "^8.16.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@olis/eslint-config": "workspace:*",
    "@olis/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
