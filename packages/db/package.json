{
  "name": "@olis/db",
  "type": "module",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "default": "./dist/schema/index.js"
    },
    "./schema/core": {
      "types": "./dist/schema/core/index.d.ts",
      "default": "./dist/schema/core/index.js"
    },
    "./schema/core/*": {
      "types": "./dist/schema/core/*.d.ts",
      "default": "./dist/schema/core/*.js"
    },
    "./schema/one-stop-sales": {
      "types": "./dist/schema/one-stop-sales/index.d.ts",
      "default": "./dist/schema/one-stop-sales/index.js"
    },
    "./schema/one-stop-sales/*": {
      "types": "./dist/schema/one-stop-sales/*.d.ts",
      "default": "./dist/schema/one-stop-sales/*.js"
    },
    "./seeds/one-stop-sales": {
      "types": "./dist/seeds/one-stop-sales/index.d.ts",
      "default": "./dist/seeds/one-stop-sales/index.js"
    },
    "./seeds/one-stop-sales/*": {
      "types": "./dist/seeds/one-stop-sales/*.d.ts",
      "default": "./dist/seeds/one-stop-sales/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./types": "./src/types/index.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "db:push": "dotenv -e .env drizzle-kit push",
    "db:seed:core": "dotenv -e .env tsx ./src/lib/db-seed.ts core",
    "db:seed:oss": "dotenv -e .env tsx ./src/lib/db-seed.ts oneStopSales",
    "db:init-schemas": "dotenv -e .env tsx ./src/lib/init-schemas.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@olis/core": "workspace:*",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "pg": "^8.16.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@olis/eslint-config": "workspace:*",
    "@olis/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
