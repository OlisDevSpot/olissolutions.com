{
  "name": "@olis/trpc",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "exports": {
    "./init": {
      "types": "./dist/init.d.ts",
      "default": "./dist/init.js"
    },
    "./routers/*": {
      "types": "./dist/routers/*.d.ts",
      "default": "./dist/routers/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.4.0",
    "@olis/auth": "workspace:*",
    "@olis/db": "workspace:*",
    "@olis/server": "workspace:*",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@trpc/tanstack-react-query": "^11.6.0",
    "client-only": "^0.0.1",
    "server-only": "^0.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@olis/types": "workspace:*",
    "@olis/typescript-config": "workspace:*",
    "@types/node": "^24.9.1",
    "@types/react": "^19",
    "eslint": "^9.37.0",
    "eslint-plugin-format": "^1.0.2",
    "react": "^19.0.0",
    "typescript": "5.9.3"
  }
}
