{
  "name": "@olis/core",
  "type": "module",
  "version": "1.0.0",
  "exports": {
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "default": "./dist/constants/index.js"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "default": "./dist/services/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./types": "./src/types/index.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@olis/eslint-config": "workspace:*",
    "@olis/typescript-config": "workspace:*",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "typescript": "^5.9.2"
  }
}
