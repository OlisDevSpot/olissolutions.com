    "@trpc/tanstack-react-query": "^11.6.0",
    "@types/react": "^19",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "superjson": "^2.2.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@olis/eslint-config": "workspace:*",
