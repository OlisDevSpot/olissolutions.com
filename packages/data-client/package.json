{
  "name": "@olis/data-client",
  "type": "module",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./get-query-client": {
      "types": "./dist/lib/get-query-client.d.ts",
      "default": "./dist/lib/get-query-client.js"
    },
    "./queries/*": {
      "types": "./dist/queries/*.d.ts",
      "default": "./dist/queries/*.js"
    },
    "./mutations/*": {
      "types": "./dist/mutations/*.d.ts",
      "default": "./dist/mutations/*.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.90.5",
    "hono": "^4.9.0"
  },
  "dependencies": {
    "@olis/core": "workspace:*",
    "@olis/db": "workspace:*",
    "@olis/server": "workspace:*",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "superjson": "^2.2.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@olis/eslint-config": "workspace:*",
    "@olis/typescript-config": "workspace:*",
    "@tanstack/react-query": "^5.90.5",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "typescript": "^5.9.2"
  }
}
