{
  "$schema": "https://schemastore.azurewebsites.net/schemas/json/package.json",
  "name": "olissolutions.com",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "check-types": "turbo check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@olis/eslint-config": "workspace:*",
    "@olis/typescript-config": "workspace:*",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.2",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.9.3",
      "lucide-react": "^0.543.0",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "@types/react": "^19",
      "@types/react-dom": "^19",
      "drizzle-orm": "^0.44.6",
      "better-auth": "^1.3.29",
      "hono": "^4.9.0",
      "@hono/zod-validator": "^0.7.2",
      "@tanstack/react-query": "^5.90.5"
    },
    "ignoredBuiltDependencies": [
      "@clerk/shared"
    ],
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "core-js-pure",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "workerd"
    ]
  }
}
